    <Grid Width="250">
        <Label Content="Login" Height="28" HorizontalAlignment="Left" Margin="32,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Haslo" Height="28" HorizontalAlignment="Left" Margin="32,46,0,0" Name="label2" VerticalAlignment="Top" />
